---
interface ButtonAttributes {
    url: string
}

const {url} = Astro.props;
---

<a href={url} class="elative min-w-[50px] border-slate-900 hover:border-slate-900/75 flex justify-center items-center overflow-hidden rounded-xl border bg-slate-800/50 w-full py-4 h-16 px-12 transition hover:bg-slate-800/75 hover:shadow-lg group">
    <slot name="beforeTitle"/>
    <slot/>
    <slot name="afterTitle"/>
</a>